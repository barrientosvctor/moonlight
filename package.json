{
  "name": "moonlight",
  "version": "1.0.0",
  "description": "Moonlight is a multi-purpose Discord bot in Spanish.",
  "main": "index.js",
  "engineStrict": true,
  "type": "module",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "build": "rimraf dist && tsc --build",
    "format:check": "prettier . --check",
    "format:write": "prettier . --write",
    "linter": "eslint .",
    "linter:fix": "eslint . --fix",
    "precommit": "npm run format:check && npm run linter"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "14.15.0",
    "humanize-duration-ts": "2.1.1",
    "lowdb": "7.0.1",
    "ms-typescript": "2.0.0",
    "rimraf": "5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "9.2.0",
    "dotenv": "16.4.5",
    "eslint": "8.57.0",
    "prettier": "3.2.5",
    "tsx": "4.9.0",
    "typescript": "5.4.5",
    "typescript-eslint": "7.8.0"
  }
}
